{
  "name": "jest-coverage-diff",
  "version": "0.0.0",
  "private": true,
  "description": "A github action to comment jest coverage diff on a PR",
  "main": "src/main.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kplates/Jest-Coverage-Diff"
  },
  "keywords": [
    "actions",
    "node",
    "jest",
    "coverage"
  ],
  "author": "Anuraag Puri",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.21",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.28.0",
    "eslint-plugin-jest": "^29.12.2",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  }
}
